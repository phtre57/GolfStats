{
  "name": "backend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && npx tsc-alias",
    "build:watch": "tsc --watch && npx tsc-alias --watch",
    "check-ts": "tsc --noEmit",
    "start": "node build/app.js",
    "dev": "concurrently \"tsc --watch\" \"npx tsc-alias --watch\" \"nodemon -q build/app.js\""
  },
  "dependencies": {
    "@sailplane/injector": "^4.0.0-alpha.1",
    "@sailplane/logger": "^4.2.0",
    "axios": "^1.3.4",
    "bottlejs": "^2.0.1",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "pg": "^8.11.1",
    "superstruct": "^1.0.3"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.2",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "tsc-alias": "^1.8.7",
    "typescript-transform-paths": "^3.4.6"
  }
}
